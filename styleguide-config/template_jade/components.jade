if components.length
  nav.conts__item
    h2.u-ttl_l Components
    ul.aigis-moduleList
      each component in components
        li.aigis-moduleList__item
          a(href='#' + component.config.name) #{component.config.name}
  each component in components
    section.conts__item
     h2.u-ttl_l(id=component.config.name) #{component.config.name}
     p!= component.sourcePath
     ul.aigis-tags
       each tag in component.config.tag
         li.aigis-tags__item(class='aigis-tags__item--' + tag) #{tag}
    section.conts__item
     h2.u-ttl_l サンプル
     .aigis-module
       != component.html

else
  != html
